{
  "author": "Florian Imdahl <git@ffflorian.de>",
  "bin": "dist/cjs/cli.js",
  "dependencies": {
    "axios": "1.6.2",
    "commander": "11.1.0",
    "find-up": "7.0.0",
    "logdown": "3.3.1",
    "open": "9.1.0"
  },
  "description": "Open a GitHub repository in your browser. Opens pull requests by default.",
  "devDependencies": {
    "http-status-codes": "2.3.0",
    "nock": "13.4.0",
    "rimraf": "5.0.5",
    "typescript": "5.3.3",
    "vitest": "0.34.6"
  },
  "engines": {
    "node": ">= 18.0"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "cli",
    "git",
    "github",
    "repository-tools",
    "typescript"
  ],
  "license": "GPL-3.0",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "name": "@ffflorian/gh-open",
  "repository": "https://github.com/ffflorian/node-packages/tree/main/packages/gh-open",
  "scripts": {
    "build": "yarn build:cjs && yarn build:esm && yarn generate:packagejson",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "dist": "yarn clean && yarn build",
    "generate:packagejson": "../../bin/generate-hybrid-package-json.sh",
    "start": "node --loader ts-node/esm src/cli.ts -d",
    "test": "vitest run"
  },
  "type": "module",
  "version": "3.5.6"
}
