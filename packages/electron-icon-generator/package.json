{
  "author": "Florian Imdahl <git@ffflorian.de>",
  "bin": {
    "electron-icon-generator": "dist/index.js"
  },
  "dependencies": {
    "commander": "10.0.0",
    "fs-extra": "11.1.0",
    "icon-gen": "3.0.1",
    "jimp": "0.16.2"
  },
  "description": "An icon generator to generate all the icon files needed for electron packaging",
  "devDependencies": {
    "@types/fs-extra": "11.0.1",
    "rimraf": "4.1.2",
    "typescript": "4.9.5"
  },
  "engines": {
    "node": ">= 10.9"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "electron",
    "icon",
    "icns",
    "png",
    "ico"
  ],
  "license": "GPL-3.0",
  "main": "dist/index.js",
  "name": "@ffflorian/electron-icon-generator",
  "publishConfig": {
    "directory": "flattened"
  },
  "repository": "https://github.com/ffflorian/node-packages/tree/main/packages/electron-icon-generator",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "dist": "yarn clean && yarn build",
    "flatten": "node ../publish-flat/dist/cli.js -o flattened",
    "postversion": "node ../../publish-flat/dist/cli-copy.js -i package.json -o ../package.json version",
    "start": "ts-node src/cli.ts",
    "test": "exit 0"
  },
  "version": "1.0.0"
}
