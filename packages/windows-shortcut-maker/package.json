{
  "author": "Pedro Costa <pedrohenriqueteodorodacosta@gmail.com>",
  "contributors": [
    "Florian Imdahl <git@ffflorian.de>"
  ],
  "description": "Create Windows shortcuts using a VBScript.",
  "devDependencies": {
    "rimraf": "5.0.5",
    "typescript": "5.4.5"
  },
  "engines": {
    "node": ">= 18.0"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist",
    "scripts"
  ],
  "keywords": [
    "windows",
    "shortcut",
    "native",
    "lightweight",
    "desktop",
    "lnk",
    "url"
  ],
  "license": "GPL-3.0",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "name": "@ffflorian/windows-shortcut-maker",
  "repository": "https://github.com/ffflorian/node-packages/tree/main/packages/windows-shortcut-maker",
  "scripts": {
    "build": "yarn build:cjs && yarn build:esm && yarn generate:packagejson",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "dist": "yarn clean && yarn build",
    "generate:packagejson": "../../bin/generate-hybrid-package-json.sh"
  },
  "type": "module",
  "version": "2.5.3"
}
