{
  "author": "Florian Imdahl <git@ffflorian.de>",
  "description": "Generate a file index",
  "devDependencies": {
    "rimraf": "4.1.2",
    "typescript": "4.9.5"
  },
  "engines": {
    "node": ">= 10.9"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "file",
    "index",
    "fileindex",
    "tree"
  ],
  "license": "GPL-3.0",
  "main": "dist/index.js",
  "name": "@ffflorian/file-index",
  "publishConfig": {
    "directory": "flattened"
  },
  "repository": "https://github.com/ffflorian/node-packages/tree/main/packages/file-index",
  "scripts": {
