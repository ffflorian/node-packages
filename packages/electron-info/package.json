{
  "author": "Florian Imdahl <git@ffflorian.de>",
  "bin": {
    "electron-info": "dist/cjs/cli.js"
  },
  "dependencies": {
    "axios": "1.6.0",
    "chalk": "5.3.0",
    "commander": "11.1.0",
    "date-fns": "2.30.0",
    "logdown": "3.3.1",
    "parse-url": "8.1.0",
    "semver": "7.5.4",
    "table": "6.8.1"
  },
  "description": "Get useful data about Electron releases.",
  "devDependencies": {
    "@types/fs-extra": "11.0.3",
    "@types/semver": "7.5.3",
    "@types/uuid": "9.0.6",
    "fs-extra": "11.1.1",
    "http-status-codes": "2.3.0",
    "nock": "13.3.7",
    "rimraf": "5.0.5",
    "typescript": "5.2.2",
    "uuid": "9.0.1"
  },
  "engines": {
    "node": ">= 14.16"
  },
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "cli",
    "info",
    "electron",
    "release",
    "version",
    "typescript"
  ],
  "license": "GPL-3.0",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "name": "electron-info",
  "repository": "https://github.com/ffflorian/node-packages/tree/main/packages/electron-info",
  "scripts": {
    "build": "yarn build:cjs && yarn build:mjs && yarn generate:packagejson",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:mjs": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "dist": "yarn clean && yarn build",
    "generate:packagejson": "../../bin/generate-hybrid-package-json.sh",
    "start": "ts-node-esm src/cli.ts -d",
    "test": "ts-node-esm -P tsconfig.jasmine.json ../../node_modules/jasmine/bin/jasmine.js"
  },
  "type": "module",
  "version": "1.20.4"
}
