{
  "author": "Florian Imdahl <git@ffflorian.de>",
  "bin": "dist/cjs/cli.js",
  "dependencies": {
    "axios": "1.6.7",
    "chalk": "5.3.0",
    "commander": "12.0.0",
    "date-fns": "3.3.1",
    "logdown": "3.3.1",
    "parse-url": "8.1.0",
    "semver": "7.6.0",
    "table": "6.8.1"
  },
  "description": "Get useful data about Electron releases.",
  "devDependencies": {
    "@types/fs-extra": "11.0.4",
    "@types/semver": "7.5.8",
    "fs-extra": "11.2.0",
    "http-status-codes": "2.3.0",
    "nock": "13.5.3",
    "rimraf": "5.0.5",
    "typescript": "5.3.3",
    "vitest": "1.3.1"
  },
  "engines": {
    "node": ">= 18.0"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "cli",
    "info",
    "electron",
    "release",
    "version",
    "typescript"
  ],
  "license": "GPL-3.0",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "name": "electron-info",
  "repository": "https://github.com/ffflorian/node-packages/tree/main/packages/electron-info",
  "scripts": {
    "build": "yarn build:cjs && yarn build:esm && yarn generate:packagejson",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "dist": "yarn clean && yarn build",
    "generate:packagejson": "../../bin/generate-hybrid-package-json.sh",
    "start": "node --loader ts-node/esm src/cli.ts -d",
    "test": "vitest run"
  },
  "type": "module",
  "version": "1.22.2"
}
