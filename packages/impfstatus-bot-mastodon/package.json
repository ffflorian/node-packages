{
  "author": "Florian Imdahl <git@ffflorian.de>",
  "bin": {
    "impfstatus": "dist/cjs/cli.js"
  },
  "dependencies": {
    "axios": "1.3.5",
    "commander": "10.0.0",
    "megalodon": "5.4.2",
    "ntfy": "1.2.0"
  },
  "description": "Toots Germany's COVID-19 vaccination progress to Mastodon.",
  "devDependencies": {
    "rimraf": "4.4.1",
    "typescript": "5.0.3"
  },
  "engines": {
    "node": ">= 14.16"
  },
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "cli",
    "typescript"
  ],
  "license": "GPL-3.0",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "name": "@ffflorian/impfstatus-bot-mastodon",
  "private": true,
  "repository": "https://github.com/ffflorian/node-packages/tree/main/packages/impfstatus-bot-mastodon",
  "scripts": {
    "build": "yarn build:cjs && yarn build:mjs && yarn generate:packagejson",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:mjs": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "dist": "yarn clean && yarn build",
    "flatten": "exit 0",
    "generate:packagejson": "../../bin/generate-hybrid-package-json.sh",
    "postversion": "exit 0",
    "start": "ts-node src/cli.ts"
  },
  "type": "module",
  "version": "1.2.0"
}
