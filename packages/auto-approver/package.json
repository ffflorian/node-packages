  },
  "engines": {
    "node": ">= 10.9"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "cli",
    "typescript"
  ],
  "license": "GPL-3.0",
  "main": "dist/index.js",
  "name": "auto-approver",
  "publishConfig": {
    "directory": "flattened"
