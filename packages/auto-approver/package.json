{
  "author": "Florian Imdahl <git@ffflorian.de>",
  "bin": "dist/cjs/cli.js",
  "dependencies": {
    "axios": "1.6.7",
    "commander": "12.0.0",
    "cosmiconfig": "9.0.0",
    "logdown": "3.3.1"
  },
  "description": "Automatically approve all GitHub PRs which match a specific pattern.",
  "devDependencies": {
    "http-status-codes": "2.3.0",
    "nock": "13.5.4",
    "rimraf": "5.0.5",
    "typescript": "5.3.3",
    "vitest": "1.3.1"
  },
  "engines": {
    "node": ">= 18.0"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "cli",
    "typescript"
  ],
  "license": "GPL-3.0",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "name": "auto-approver",
  "repository": "https://github.com/ffflorian/node-packages/tree/main/packages/auto-approver",
  "scripts": {
    "build": "yarn build:cjs && yarn build:esm && yarn generate:packagejson",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "dist": "yarn clean && yarn build",
    "generate:packagejson": "../../bin/generate-hybrid-package-json.sh",
    "start": "node --loader ts-node/esm src/cli.ts",
    "test": "vitest run"
  },
  "type": "module",
  "version": "1.13.2"
}
