{
  "author": "Florian Imdahl <git@ffflorian.de>",
  "bin": "dist/cjs/cli.js",
  "dependencies": {
    "basic-auth": "2.0.1",
    "commander": "12.0.0",
    "http-status-codes": "2.3.0",
    "logdown": "3.3.1",
    "tsscmp": "1.0.6"
  },
  "description": "A simple HTTPS proxy for Node.js with authentication support.",
  "devDependencies": {
    "@types/basic-auth": "1.1.8",
    "@types/tsscmp": "1.0.2",
    "rimraf": "5.0.5",
    "typescript": "5.4.4"
  },
  "engines": {
    "node": ">= 18.0"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "cli",
    "typescript"
  ],
  "license": "GPL-3.0",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "name": "@ffflorian/https-proxy",
  "repository": "https://github.com/ffflorian/node-packages/tree/main/packages/https-proxy",
  "scripts": {
    "build": "yarn build:cjs && yarn build:esm && yarn generate:packagejson",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "dist": "yarn clean && yarn build",
    "generate:packagejson": "../../bin/generate-hybrid-package-json.sh",
    "start": "node --loader ts-node/esm src/cli.ts"
  },
  "type": "module",
  "version": "1.10.2"
}
