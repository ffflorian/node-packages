{
  "author": "Florian Imdahl <git@ffflorian.de>",
  "dependencies": {
    "jszip": "3.10.1",
    "protobufjs": "7.2.3"
  },
  "description": "Temporary Exposure Keys (TEK) library",
  "devDependencies": {
    "protobufjs-cli": "1.1.1",
    "rimraf": "4.4.1",
    "typescript": "5.0.3"
  },
  "engines": {
    "node": ">= 14.16"
  },
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist",
    "proto/export.js",
    "proto/export.d.ts"
  ],
  "keywords": [
    "tek",
    "temporary",
    "exposure",
    "keys",
    "google",
    "apple",
    "corona",
    "covid",
    "covid19",
    "covid-19",
    "typescript"
  ],
  "license": "GPL-3.0",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "name": "exposure-keys",
  "repository": "https://github.com/ffflorian/node-packages/tree/main/packages/exposure-keys",
  "scripts": {
    "build": "yarn build:proto && build:cjs && yarn build:mjs && yarn generate:packagejson",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:mjs": "tsc -p tsconfig.json",
    "build:proto": "pbjs --t static-module -w commonjs -o proto/export.js proto/export.proto && pbts --out proto/export.d.ts --no-comments proto/export.js",
    "clean": "rimraf dist",
    "dist": "yarn clean && yarn build",
    "generate:packagejson": "../../bin/generate-hybrid-package-json.sh",
    "start": "ts-node src/cli.ts",
    "test": "ts-node -P tsconfig.jasmine.json ../../node_modules/.bin/jasmine"
  },
  "type": "module",
  "version": "1.5.0"
}
